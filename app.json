  {
    "expo": {
      "name": "DFM",
      "slug": "DFM",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/DFMlogo.png",
      "userInterfaceStyle": "light",
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/DFMlogo.png",
          "backgroundColor": "#ffffff"
        },
        "edgeToEdgeEnabled": true,
        "predictiveBackGestureEnabled": false,
        "package": "com.anonymous.DFM",
        "permissions": [
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION"
        ]
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "plugins": [
        "expo-sqlite",
        "expo-font",
        "expo-location"
      ],
      "ios": {
        "supportsTablet": true,
        "infoPlist": {
          "NSLocationWhenInUseUsageDescription": "La aplicación necesita su ubicación para autocompletar las coordenadas del prevuelo."
        }
      }
    }
  }
